{
  "name": "frontendgrocer",
  "version": "1.0.0-beta.0",
  "description": "A grocery shopping progressive web app",
  "main": "server/cli/cli.js",
  "repository": "https://github.com/mike-north/pwa-workshop.git",
  "contributors": [
    "Mike North <michael.l.north@gmail.com> (https://mike.works)",
    "Steve Kinney <hello@stevekinney.net> (https://stevekinney.net)"
  ],
  "engines": {
    "node": ">= 6.11.5"
  },
  "private": true,
  "license": "SEE LICENSE IN LICENSE and LICENSE-exercises",
  "scripts": {
    "watch:api": "./node_modules/.bin/nodemon -e js --watch ./server/api-server server/api-server/index.js",
    "watch:ui": "./node_modules/.bin/nodemon -e js --watch ./server/asset-server server/asset-server/index.js",
    "watch": "concurrently --names 'API,UI' -c 'bgBlue,bgMagenta' 'npm run watch:api' 'npm run watch:ui'",
    "start:api": "node server/api-server/index.js",
    "start:ui": "node server/asset-server/index.js",
    "start": "concurrently --names 'API,UI' -c 'bgBlue,bgMagenta' 'npm run start:api' 'npm run start:ui'",
    "build:dev": " ./node_modules/.bin/webpack --config webpack.config.js --env dev",
    "build:prod": " ./node_modules/.bin/webpack --config webpack.config.js --env prod",
    "clean": "rm -rf dist/* db/development.sqlite && npm run prepcerts",
    "test": "jest",
    "prepvapid": "web-push generate-vapid-keys --json > ./private/vapid.json",
    "prepcerts": "npm run prepvapid && node ./server/gen-certs.js && rm -rf dist/*.pem && mkdir -p dist && cp private/*.pem dist/",
    "heroku-postbuild": "npm run build:prod",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@types/express": "^4.17.8",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/sequelize": "^4.28.9",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^5.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "greenkeeper-lockfile": "^1.15.1",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.11.2",
    "muicss": "^0.10.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "qrcode-reader": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^10.0.2",
    "style-ext-html-webpack-plugin": "^4.1.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "worker-loader": "^3.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "devcert-with-localhost": "^0.3.3",
    "express": "^4.17.1",
    "idb": "^5.0.5",
    "moment": "^2.28.0",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "web-push": "^3.4.4"
  }
}
